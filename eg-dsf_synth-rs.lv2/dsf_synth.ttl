@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .

<https://github.com/Ninja-Koala/dsf-synth>
	a lv2:Plugin ;
	doap:name "Discrete summation formula synthesizer" ;
	doap:license <http://opensource.org/licenses/isc> ;
	lv2:project <https://github.com/Ninja-Koala/dsf-synth> ;
	lv2:requiredFeature urid:map , lv2:inPlaceBroken ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:InputPort ,
				lv2:ControlPort ;
		lv2:index 0 ;
		lv2:symbol "gain" ;
		lv2:name "Attack" ;
		lv2:default 32 ;
		lv2:minimum 0 ;
		lv2:maximum 127 ;
		lv2:portProperty lv2:integer;
	] , [
		a lv2:InputPort ,
				lv2:ControlPort ;
		lv2:index 1 ;
		lv2:symbol "gain" ;
		lv2:name "Decay" ;
		lv2:default 32 ;
		lv2:minimum 0 ;
		lv2:maximum 127 ;
		lv2:portProperty lv2:integer;
	] , [
		a lv2:InputPort ,
				lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "gain" ;
		lv2:name "Sustain" ;
		lv2:default 64 ;
		lv2:minimum 0 ;
		lv2:maximum 127 ;
		lv2:portProperty lv2:integer;
	] , [
		a lv2:InputPort ,
				lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "gain" ;
		lv2:name "Release" ;
		lv2:default 32 ;
		lv2:minimum 0 ;
		lv2:maximum 127 ;
		lv2:portProperty lv2:integer;
	] , [
		a lv2:InputPort ,
				lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "gain" ;
		lv2:name "Brightness" ;
		lv2:default 64 ;
		lv2:minimum 0 ;
		lv2:maximum 127 ;
		lv2:portProperty lv2:integer;
	] , [
		a lv2:InputPort ,
				lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "gain" ;
		lv2:name "Gain" ;
		lv2:default 0 ;
		lv2:minimum -90 ;
		lv2:maximum 24 ;
		lv2:portProperty lv2:integer;
	] , [
		a lv2:InputPort ,
				lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "gain" ;
		lv2:name "Input channel" ;
		lv2:default 1 ;
		lv2:minimum 1 ;
		lv2:maximum 16 ;
		lv2:portProperty lv2:integer;
	] , [
		a lv2:InputPort ,
			atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports midi:MidiEvent ;
		lv2:index 7 ;
		lv2:symbol "in" ;
		lv2:name "MIDI In"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 8 ;
		lv2:symbol "out" ;
		lv2:name "Audio Out Left"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 9 ;
		lv2:symbol "out" ;
		lv2:name "Audio Out Right"
	] .
